   "source": [
    "from sympy import *\n",
    "x, y = symbols(\"x, y\")\n",
    "f = x * sin(y) + y * cos(x)\n",
    "dfx = diff(f, x)\n",
    "dfy = diff(f, y)\n",
    "print(-dfx / dfy)\n",
    "print(idiff(f, y, x))"
   ]
  }
