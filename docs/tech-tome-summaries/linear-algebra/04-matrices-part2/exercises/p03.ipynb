    "import numpy as np\n",
    "\n",
    "m = np.random.randn(10, 10)\n",
    "r0 = np.sqrt(np.sum(m**2))\n",
    "r1 = np.sqrt(np.trace(m @ m.T))\n",
    "r2 = np.sqrt(np.trace(m.T @ m))\n",
    "\n",
    "print(np.allclose(r0, r1))\n",
    "print(np.allclose(r1, r2))\n",
    "print(np.allclose(r0, r2))"
   ]
  }
