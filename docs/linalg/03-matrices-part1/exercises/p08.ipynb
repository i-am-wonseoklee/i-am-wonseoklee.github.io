   "source": [
    "import numpy as np\n",
    "\n",
    "def is_symmetric(A, epsilon = 1e-6):\n",
    "    sq_err = (A - A.T)**2\n",
    "    return np.sum(sq_err) < epsilon"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "False\n",
      "True\n"
     ]
    }
   ],
   "source": [
    "A = np.random.randn(5, 5)\n",
    "S = A @ A.T\n",
    "\n",
    "print(is_symmetric(A))\n",
    "print(is_symmetric(S))"
   ]
  }
