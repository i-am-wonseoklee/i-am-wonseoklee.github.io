   "source": [
    "R = np.array([1.0, 0.2, 0.9, 0.2, 1.0, 0.3, 0.9, 0.3, 1.0]).reshape(3, 3)\n",
    "evals, evecs = np.linalg.eig(R)\n",
    "X = np.random.randn(3, 10000)\n",
    "Y = evecs @ np.diag(evals)**0.5 @ X\n",
    "Rr = np.corrcoef(Y)\n",
    "print(np.round(R - Rr, 6))"
   ]
  }
