      "(-3*x*z - 3*y**2)/(3*x*y + 3*z**2)\n",
      "-(x*z + y**2)/(x*y + z**2)\n"
     ]
    }
   ],
   "source": [
    "from sympy import *\n",
    "x, y, z = symbols(\"x, y, z\")\n",
    "f = x**3 + y**3 + z**3 + 3 * x * y * z\n",
    "dfy = diff(f, y)\n",
    "dfz = diff(f, z)\n",
    "print(-dfy / dfz)\n",
    "print(idiff(f, z, y))"
   ]
  }
