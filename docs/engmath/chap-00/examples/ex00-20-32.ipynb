   "source": [
    "from sympy import *\n",
    "r, theta = symbols(\"x, theta\")\n",
    "x = r * cos(theta)\n",
    "y = r * sin(theta)\n",
    "f = sqrt(4 * x**2 + 4 * y**2 + 1)\n",
    "ans = integrate(f * r, (r, 0, 4), (theta, 0, 2 * pi))\n",
    "print(ans)"
   ]
  }
